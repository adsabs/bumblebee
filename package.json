  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "NODE_ENV=production docker compose --project-directory nginx up --build dev",
    "start:qa": "NODE_ENV=production docker compose --project-directory nginx up --build qa",
    "start:prod": "NODE_ENV=production docker compose --project-directory nginx up --build prod",
    "dev": "NODE_ENV=development webpack serve --mode development --open --hot",
    "build:release": "NODE_ENV=production webpack",
    "build": "NODE_ENV=production LOCAL_BUILD=1 webpack",
    "upload:assets": "npx wrangler pages deploy dist --project-name=ads-assets --branch=master --skip-caching --upload-source-maps && pnpm run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org adsabs --project bumblebee ./dist && sentry-cli sourcemaps upload --org adsabs --project bumblebee ./dist"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
