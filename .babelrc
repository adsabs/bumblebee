{
	"plugins": [
		"transform-react-jsx",
		["transform-object-rest-spread", { "useBuiltIns": true }]
	],
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": [
						"last 2 versions",
						"ios_saf >= 8",
						"not IE <= 10",
						"chrome >= 49",
						"firefox >= 49",
						"> 1%"
					]
				},
				"debug": false,
				"loose": true,
				"modules": false,
				"useBuiltIns": true
			}
		],
		"react"
	]
}
