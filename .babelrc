{
	"plugins": [
		"@babel/plugin-transform-react-jsx-compat",
		"@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-async-to-generator"
	],
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"last 2 versions",
						"ios_saf >= 8",
						"not IE <= 10",
						"chrome >= 49",
						"firefox >= 49",
						"> 1%"
					]
				},
				"debug": false,
				"loose": true,
        "modules": false,
        "useBuiltIns": false
			}
		],
		"@babel/preset-react"
	]
}
